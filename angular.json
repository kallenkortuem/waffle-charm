{
  "version": 1,
  "projects": {
    "wild-hunt": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/wild-hunt",
      "sourceRoot": "apps/wild-hunt/src",
      "prefix": "waffle-charm",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/wild-hunt",
            "index": "apps/wild-hunt/src/index.html",
            "main": "apps/wild-hunt/src/main.ts",
            "polyfills": "apps/wild-hunt/src/polyfills.ts",
            "tsConfig": "apps/wild-hunt/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/wild-hunt/src/favicon.ico",
              "apps/wild-hunt/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/wild-hunt/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/wild-hunt/src/environments/environment.ts",
                  "with": "apps/wild-hunt/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "wild-hunt:build",
            "proxyConfig": "apps/wild-hunt/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "wild-hunt:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wild-hunt:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/wild-hunt/tsconfig.app.json",
              "apps/wild-hunt/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/wild-hunt/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/wild-hunt/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "wild-hunt-e2e": {
      "root": "apps/wild-hunt-e2e",
      "sourceRoot": "apps/wild-hunt-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/wild-hunt-e2e/cypress.json",
            "tsConfig": "apps/wild-hunt-e2e/tsconfig.e2e.json",
            "devServerTarget": "wild-hunt:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "wild-hunt:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/wild-hunt-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/wild-hunt-e2e/**/*"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/api/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/api-interfaces/tsconfig.lib.json",
              "libs/api-interfaces/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/api-interfaces/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/api-interfaces/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "crow-storm": {
      "root": "apps/crow-storm",
      "sourceRoot": "apps/crow-storm/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/crow-storm",
            "index": "apps/crow-storm/src/index.html",
            "main": "apps/crow-storm/src/main.tsx",
            "polyfills": "apps/crow-storm/src/polyfills.ts",
            "tsConfig": "apps/crow-storm/tsconfig.app.json",
            "assets": [
              "apps/crow-storm/src/favicon.ico",
              "apps/crow-storm/src/assets"
            ],
            "styles": ["apps/crow-storm/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/crow-storm/src/environments/environment.ts",
                  "with": "apps/crow-storm/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "crow-storm:build",
            "proxyConfig": "apps/crow-storm/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "crow-storm:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/crow-storm/tsconfig.app.json",
              "apps/crow-storm/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/crow-storm/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/crow-storm/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "crow-storm-e2e": {
      "root": "apps/crow-storm-e2e",
      "sourceRoot": "apps/crow-storm-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/crow-storm-e2e/cypress.json",
            "tsConfig": "apps/crow-storm-e2e/tsconfig.e2e.json",
            "devServerTarget": "crow-storm:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "crow-storm:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/crow-storm-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/crow-storm-e2e/**/*"]
          }
        }
      }
    },
    "doctor-manhattan": {
      "root": "apps/doctor-manhattan",
      "sourceRoot": "apps/doctor-manhattan",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/doctor-manhattan",
            "outputPath": "dist/apps/doctor-manhattan"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "doctor-manhattan:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "doctor-manhattan:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "doctor-manhattan:build:production",
            "proxyConfig": "apps/doctor-manhattan/proxy.conf.json"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/doctor-manhattan/tsconfig.json",
              "apps/doctor-manhattan/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/doctor-manhattan/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/doctor-manhattan/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "doctor-manhattan-e2e": {
      "root": "apps/doctor-manhattan-e2e",
      "sourceRoot": "apps/doctor-manhattan-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/doctor-manhattan-e2e/cypress.json",
            "tsConfig": "apps/doctor-manhattan-e2e/tsconfig.e2e.json",
            "devServerTarget": "doctor-manhattan:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "doctor-manhattan:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/doctor-manhattan-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/doctor-manhattan-e2e/**/*"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "wild-hunt"
}
